{
    "name": "kherge/elf",
    "description": "A collection of helpers for the Symfony Console component.",
    "keywords": ["symfony", "console", "helpers"],
    "homepage": "http://github.com/kherge/Elf",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/herrera-io/php-cli-app/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "justinrainbow/json-schema": "1.1.*",
        "seld/jsonlint": "1.*",
        "symfony/console": "~2.1",
        "symfony/process": "~2.1"
    },
    "require-dev": {
        "herrera-io/phpunit-test-case": "1.*",
        "mikey179/vfsStream": "1.1.0",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "KevinGH\\Elf": "src/lib/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
